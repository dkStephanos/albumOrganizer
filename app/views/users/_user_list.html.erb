<div>
    <h1>Users</h1>
    <% @users.each do |user| %>
    <li>
       	<% if user.id != current_user.id && !current_user.sent_access_requests.find {|r| r.requested_id == user.id } %>
       		<%=	link_to "Request Access", :controller => "access_requests", :action => "new", :userId => user.id, :currentUserId => current_user.id  %>
       		       	 : 
       	<% end %>
       	<%= link_to user.email, user_path(user) %>
    </li>
<% end %>

</div>

